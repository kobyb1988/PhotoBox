<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="ImageMaker.Runner.AppService" behaviorConfiguration="defBehavior">
        <endpoint
          address="net.tcp://localhost:8732/ImageMaker.Runner.AppService"
     binding="netTcpBinding"
     bindingConfiguration="ultra"
     contract="ImageMaker.AppServer.ICallingContract"/>
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding name="ultra"
             maxBufferPoolSize="1048576"
             maxBufferSize="1048576"
             maxConnections="1048576"
             maxReceivedMessageSize="1048576"
             portSharingEnabled="false"
             transactionFlow="false"
             listenBacklog="1048576">
          <security mode="None">
            <message clientCredentialType="None"/>
            <transport protectionLevel="None" clientCredentialType="None"/>
          </security>
          <reliableSession enabled="true"/>
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="defBehavior">
          <!-- To avoid disclosing metadata information, 
          set the values below to false before deployment -->
          <serviceMetadata/>
          <!-- To receive exception details in faults for debugging purposes, 
          set the value below to true.  Set to false before deployment 
          to avoid disclosing exception information -->
          <serviceDebug includeExceptionDetailInFaults="True" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>